<Page
    navigatingTo="onNavigatingTo"
    xmlns:maps="nativescript-google-maps-sdk">

    <ActionBar class="maps">
        <Label text="Maps"></Label>
    </ActionBar>

    <GridLayout>
        <maps:mapView
            latitude="{{ latitude }}"
            longitude="{{ longitude }}"
            zoom="{{ zoom }}"
        />
    </GridLayout>
</Page>

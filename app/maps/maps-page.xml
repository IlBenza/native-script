<Page
    navigatingTo="onNavigatingTo"
    xmlns="http://schemas.nativescript.org/tns.xsd">

    <ActionBar class="maps">
        <Label text="Maps"></Label>
    </ActionBar>

    <StackLayout orientation="vertical">
        <TextField
            hint="Enter nome"
            text="{{ nome }}"
            color="orangered"
            backgroundColor="#ffffff"></TextField>
        <TextField
            hint="Enter cognome"
            text="{{ cognome }}"
            color="orangered"
            backgroundColor="#ffffff"></TextField>
        <Label text="{{ nome }}"></Label>
        <Label text="{{ cognome }}"></Label>
    </StackLayout>
</Page>
